{
  "name": "@dgshahr/ui-kit",
  "version": "0.0.2",
  "description": "dgshahr ui-kit",
  "author": "dgshahr",
  "license": "MIT",
  "type": "module",
  "repository": {
    "url": "https://github.com/dgshahr/uikit"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsc && vite build && node scripts/generate-exports.js",
    "build:story": "storybook build",
    "lint": "eslint --fix src/**/* --report-unused-disable-directives --max-warnings 0",
    "format": "prettier src/**/* --write --ignore-unknown",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@dgshahr/dg-theme-tailwind-plugin": "^1.0.3",
    "@eslint/compat": "^1.1.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.11.1",
    "@storybook/addon-actions": "^8.3.2",
    "@storybook/addon-essentials": "^8.3.2",
    "@storybook/blocks": "^8.3.2",
    "@storybook/builder-vite": "^8.3.2",
    "@storybook/react": "^8.3.2",
    "@storybook/react-vite": "^8.3.2",
    "@types/node": "^22.5.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.3.2",
    "strip-ansi": "6",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^4.2.1",
    "vite-plugin-lib-inject-css": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "exports": {
    "./Accordion": {
      "import": {
        "types": "./dist/Accordion/index.d.ts",
        "default": "./dist/Accordion/index.js"
      },
      "require": {
        "types": "./dist/Accordion/index.d.ts",
        "default": "./dist/Accordion/index.cjs"
      }
    },
    "./Alert": {
      "import": {
        "types": "./dist/Alert/index.d.ts",
        "default": "./dist/Alert/index.js"
      },
      "require": {
        "types": "./dist/Alert/index.d.ts",
        "default": "./dist/Alert/index.cjs"
      }
    },
    "./Badge": {
      "import": {
        "types": "./dist/Badge/index.d.ts",
        "default": "./dist/Badge/index.js"
      },
      "require": {
        "types": "./dist/Badge/index.d.ts",
        "default": "./dist/Badge/index.cjs"
      }
    },
    "./Breadcrumb": {
      "import": {
        "types": "./dist/Breadcrumb/index.d.ts",
        "default": "./dist/Breadcrumb/index.js"
      },
      "require": {
        "types": "./dist/Breadcrumb/index.d.ts",
        "default": "./dist/Breadcrumb/index.cjs"
      }
    },
    "./Button": {
      "import": {
        "types": "./dist/Button/index.d.ts",
        "default": "./dist/Button/index.js"
      },
      "require": {
        "types": "./dist/Button/index.d.ts",
        "default": "./dist/Button/index.cjs"
      }
    },
    "./Divider": {
      "import": {
        "types": "./dist/Divider/index.d.ts",
        "default": "./dist/Divider/index.js"
      },
      "require": {
        "types": "./dist/Divider/index.d.ts",
        "default": "./dist/Divider/index.cjs"
      }
    },
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./Pagination": {
      "import": {
        "types": "./dist/Pagination/index.d.ts",
        "default": "./dist/Pagination/index.js"
      },
      "require": {
        "types": "./dist/Pagination/index.d.ts",
        "default": "./dist/Pagination/index.cjs"
      }
    },
    "./Slider": {
      "import": {
        "types": "./dist/Slider/index.d.ts",
        "default": "./dist/Slider/index.js"
      },
      "require": {
        "types": "./dist/Slider/index.d.ts",
        "default": "./dist/Slider/index.cjs"
      }
    }
  }
}