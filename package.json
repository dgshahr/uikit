{
  "name": "@dgshahr/ui-kit",
  "version": "0.0.95",
  "description": "A modern, accessible, and customizable React component library",
  "author": "dgshahr",
  "contributes": [
    {
      "name": "Mohammad Nazari",
      "email": "mohammadnazary2000@gmail.com",
      "url": "https://github.com/nazari2079"
    }
  ],
  "license": "MIT",
  "type": "module",
  "homepage": "https://dgshahr.github.io/uikit/",
  "keywords": [
    "ui-kit",
    "component",
    "components",
    "design",
    "framework",
    "frontend",
    "react",
    "react-component",
    "ui"
